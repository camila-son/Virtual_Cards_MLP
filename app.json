{
  "expo": {
    "name": "Virtual Cards MLP",
    "slug": "virtual-cards-mlp",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "fonts": [
      "assets/fonts/*.ttf",
      "assets/fonts/*.otf"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.virtualcardsmlp.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ece9ee"
      }
    },
    "plugins": [
      "expo-font",
      "expo-asset"
    ]
  }
}
